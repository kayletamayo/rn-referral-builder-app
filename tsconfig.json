{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@screens/*": ["src/screens/*"],
      "@screens": ["src/screens"],
      "@navigation/*": ["src/navigation/*"],
      "@navigation": ["src/navigation"],
      "@utils/*": ["src/utils/*"],
      "@utils": ["src/utils"],
      "@defaults/*": ["src/defaults/*"],
      "types/*": ["src/types/*"],
      "@constants/*": ["src/constants/*"],
      "@hooks/*": ["src/hooks/*"],
      "@context/*": ["src/context/*"],
    }
  }
}
